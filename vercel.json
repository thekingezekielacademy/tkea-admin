{
  "buildCommand": "CI=false npm run build:admin",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/create-batch",
      "schedule": "0 0 * * 1-5"
    },
    {
      "path": "/api/cron/generate-batch-sessions",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/live-booth-reminders",
      "schedule": "0 0 * * *"
    }
  ]
}