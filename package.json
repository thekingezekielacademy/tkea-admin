{
  "name": "king-ezekiel-academy",
  "version": "1.0.0",
  "description": "King Ezekiel Academy - Next.js + Node.js + Supabase Application",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "cd king-ezekiel-academy-nextjs && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd king-ezekiel-academy-nextjs && npm run build",
    "install-client": "cd king-ezekiel-academy-nextjs && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "pre-deploy": "./scripts/pre-deploy.sh",
    "deploy": "./scripts/deploy.sh",
    "build-check": "npm run build && echo 'âœ… Build check passed!'",
    "test-deployment": "./scripts/test-deployment.sh",
    "health-check": "./scripts/health-check.sh",
    "rollback": "./scripts/rollback.sh",
    "safe-deploy": "npm run test-deployment && npm run build && git add . && git commit -m 'Safe deployment' && git push origin main && npx vercel --prod && npm run health-check"
  },
  "keywords": [
    "education",
    "academy",
    "nextjs",
    "nodejs",
    "supabase"
  ],
  "author": "King Ezekiel Academy",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "@vercel/node": "^4.0.0",
    "and": "^0.0.3",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "start": "^5.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "next": "15.5.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  }
}
