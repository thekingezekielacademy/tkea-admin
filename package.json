{
  "name": "king-ezekiel-academy",
  "version": "1.0.0",
  "description": "King Ezekiel Academy - Next.js + Node.js + Supabase Application",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "react-start": "PORT=3001 react-scripts start",
    "server": "nodemon server/index.js",
    "server:5001": "PORT=5001 nodemon server/index.js",
    "client": "cd king-ezekiel-academy-nextjs && npm run dev",
    "client:3001": "cd king-ezekiel-academy-nextjs && next dev -p 3001",
    "client:4000": "cd king-ezekiel-academy-nextjs && next dev -p 4000",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:3001": "concurrently \"npm run server\" \"npm run client:3001\"",
    "dev:4000": "concurrently \"npm run server\" \"npm run client:4000\"",
    "dev:5001": "concurrently \"npm run server:5001\" \"npm run client\"",
    "dev:custom": "concurrently \"npm run server:5001\" \"npm run client:3001\"",
    "build": "cd king-ezekiel-academy-nextjs && npm run build",
    "build:admin": "CI=false react-scripts build",
    "install-client": "cd king-ezekiel-academy-nextjs && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "pre-deploy": "./scripts/pre-deploy.sh",
    "deploy": "./scripts/deploy.sh",
    "build-check": "npm run build && echo 'âœ… Build check passed!'",
    "test-deployment": "./scripts/test-deployment.sh",
    "health-check": "./scripts/health-check.sh",
    "rollback": "./scripts/rollback.sh",
    "safe-deploy": "npm run test-deployment && npm run build && git add . && git commit -m 'Safe deployment' && git push origin main && npx vercel --prod && npm run health-check"
  },
  "keywords": [
    "education",
    "academy",
    "nextjs",
    "nodejs",
    "supabase"
  ],
  "author": "King Ezekiel Academy",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "@types/papaparse": "^5.5.2",
    "@types/react-router-dom": "^5.3.3",
    "@vercel/node": "^4.0.0",
    "and": "^0.0.3",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "react-scripts": "^5.0.1",
    "recharts": "^3.4.1",
    "start": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "next": "15.5.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
