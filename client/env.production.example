# Production Environment Variables for Vercel
# Copy this to .env.production and fill in your values
REACT_APP_SUPABASE_URL=your_production_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_production_supabase_anon_key
REACT_APP_API_URL=https://your-vercel-domain.vercel.app/api
GENERATE_SOURCEMAP=false

# Flutterwave Live Mode
REACT_APP_FLUTTERWAVE_PUBLIC_KEY=your_flutterwave_client_id
REACT_APP_FLUTTERWAVE_SECRET_KEY=your_flutterwave_client_secret
REACT_APP_FLUTTERWAVE_ENCRYPTION_KEY=your_flutterwave_encryption_key
REACT_APP_FLUTTERWAVE_MODE=live
