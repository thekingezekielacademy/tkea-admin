  "scripts": {
    "start": "react-scripts start",
    "build": "node build-simple.js",
    "build:react": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build:es5": "npm run build:es5:clean && npm run build:es5:webpack && npm run build:es5:copy",
    "build:es5:clean": "rm -rf build-es5 && mkdir -p build-es5/static/js build-es5/static/css build-es5/static/media",
    "build:es5:webpack": "webpack --config webpack.config.es5.js --mode production",
    "build:es5:copy": "cp -r public/* build-es5/ && cp public/index.es5.html build-es5/index.html",
    "build:all": "npm run build && npm run build:es5",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "safari >= 11",
      "ios_saf >= 11",
      "chrome >= 58",
      "firefox >= 58",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "safari >= 11",
      "ios_saf >= 11"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
